/**
 * Implementation of hook_uninstall().
 */
function cck_plan_fields_uninstall() {
  variable_del('cck_plan_fields_vid');
}

/**
 * Implementation of cck_plan_fields_enable().
 */
function cck_plan_fields_enable() {
  drupal_load('module', 'taxonomy');
  $vocabulary = taxonomy_vocabulary_load(variable_get('cck_plan_fields_vid', 0));
  if (!$vocabulary) {
   // Create the Cuentas Presupuestarias vocabulary if it does not exist. Assign the vocabulary
   // a low weight so it will appear first in forum topic create and edit
   // forms.
   $vocabulary = array(
      'name' => t('Cuentas Presupuestarias'),
      'multiple' => 0,
      'required' => 0,
      'hierarchy' => 1,
      'relations' => 0,
      'module' => 'cck_plan_fields',
      'weight' => -10,
    );
    taxonomy_save_vocabulary($vocabulary);
    variable_set('cck_plan_fields_vid', $vocabulary['vid']);
    _cck_plan_fields_save_term($vocabulary['vid']);
  }
}

